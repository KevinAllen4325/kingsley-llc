	<!-- above the fold -->
    <style>		
        :root{
			--primary-green:#1B892F;
			--faded-green:#5EAC6C;
			--black:#1c242a;
			--textColor:#7e7e7e 
        }
        body{
            font-family:sans-serif;
            font-size:16px;
            line-height:1.5;
            color:var(--black);
            letter-spacing:.5px 
        }
        :focus{
            outline:0!important 
        }
        h1,h2,h3,h4,h5,h6{
            font-family: 'Roboto', sans-serif;
            font-weight: 700;
        }
        p {
            color: var(--textColor);
            letter-spacing: 1px;
        }
        p strong{
            color: var(--black);
        }
        a{
            text-decoration:none;
            color:var(--primary-green);
            outline:0!important 
        }
        .font-bold{
            font-weight: 700;
        }
        a:active,a:focus{
            outline:0!important 
        }
        .container{
            position:relative;
            max-width:1140px;
            width:100%;
            padding:0 20px;
            margin:0 auto 
        }
        .flex-center{
            display:flex;
            justify-content:center;
            align-items:center 
        }
        .flex-space-top{
            display:flex;
            justify-content:space-between;
            align-items:top 
        }
        .flex-space-between{
            display:flex;
            justify-content:space-between;
            align-items:center 
        }
        .border-bottom{
            border-bottom:1px solid #1c242a1f 
        }
        .section-padding{
            padding-top:60px;
            padding-bottom:60px 
        }
        header{
            background-color:#fff;
            padding:23px 0;
            position:relative 
        }
        .logged-in header.fixed-nav{
            top:32px 
        }
        header .container a{
            height:34px 
        }
        header .container a img{
            width:200px 
        }
        header #header-call-now{
            font-size:16px;
            height:auto;
            color:var(--black);
            transition:color .3s ease-in-out 
        }
        header #header-call-now i{
            font-size:16px;
            font-weight: 700;
            margin-right:10px 
        }
        header #header-call-now:hover{
            color:var(--primary-green) 
        }
        header #menuIcon{
            margin-left:20px;
            color:var(--primary-green);
            cursor:pointer;
            font-size:30px 
        }
        header .navIcons{
            display:flex;
            align-items:center 
        }
        .homepage-hero{
            min-height:calc(70vh - 81px);
            padding:30px 0;
            background:no-repeat center;
            background-size:cover;
            position:relative 
        }
        .logged-in .homepage-hero{
            min-height:calc(70vh - 112px);
            position:relative 
        }
        .homepage-hero *{
            text-align:center 
        }
        .leftBar,.rightBar{
            position:absolute;
            top:0;
            width:20px;
            background-color:var(--primary-green);
            z-index:5 
        }
        .homepage-hero .leftBar,.homepage-hero .rightBar{
            height:calc(100% + 1px) 
        }
        .page-header .leftBar,.page-header .rightBar{
            height:100% 
        }
        .leftBar{
            left:0 
        }
        .rightBar{
            right:0 
        }
        .homepage-hero .overlay-black{
            position:absolute;
            top:0;
            left:0;
            width:100%;
            height:100%;
            background-color:rgba(0,0,0,.2) 
        }
        .homepage-hero .hero-overlay{
            background:rgba(255,255,255,.6);
            padding:30px 50px;
            border-radius:4px;
            backdrop-filter:blur(15px);
            max-width:470px;
            margin:0 auto 
        }
        .homepage-hero .hero-image{
            width:80px;
            height:80px;
            margin:0 auto;
            border-radius:100%;
            overflow:hidden 
        }
        .homepage-hero .hero-image img{
            width:82px;
            height: auto;
        }
        .homepage-hero .sub-location{
            color:#008b19;
            font-size:14px;
            margin:10px auto 
        }
        .homepage-hero h1{
            font-size:40px;
            font-weight:600;
            max-width:370px;
            margin:0 auto;
            line-height:1.1 
        }
        .call-to-action{
			display: inline-flex;
			background-color: var(--primary-green);
			padding: 12px 35px;
			color: #fff;
			font-size: 20px;
			min-width: 312px;
			border-radius: 2rem;
			margin: 20px 0;
			animation: pulse 1.5s infinite;
        }
        .call-to-action:hover{
            animation:none 
        }
        @keyframes pulse{
            0%{
                transform:scale(.95) 
            }
            70%{
                transform:scale(1) 
            }
            100%{
                transform:scale(.95) 
            }
        }
        .call-to-action i{
            font-size:15px 
        }
        .call-to-action.static{
            display:none;
            margin:0 auto;
            text-align:center 
        }
        .homepage-hero .hero-sub-header{
            font-size:16px;
            font-family: sans-serif;
            font-weight: 400;
            letter-spacing: 1px;
            margin:10px auto 
        }
        .homepage-hero .hero-locations{
            font-size:16px;
            color: var(--black);
            max-width:225px;
            margin:10px auto 0 
        }
	</style>
    <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen" />